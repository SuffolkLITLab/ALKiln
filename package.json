{
  "name": "docassemble-cucumber",
  "version": "0.1.5",
  "description": "Integrated automated end-to-end testing with docassemble, puppeteer, and cucumber.",
  "main": "index.js",
  "scripts": {
    "test": "echo 'These tests are fake. Use `local` or `cucumber`' && mocha tests --timeout 10000",
    "cucumber": "./node_modules/.bin/cucumber-js tests/features/*.feature",
    "setup": "node -e ./setup.js",
    "takedown": "node -e 'require(\"./tests/install-branch.js\").takedown()'",
    "local": "npm run setup && npm run cucumber; npm run takedown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plocket/docassemble-cucumber.git"
  },
  "keywords": [
    "docassemble",
    "cucumber",
    "puppeteer"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plocket/docassemble-cucumber/issues"
  },
  "homepage": "https://github.com/plocket/docassemble-cucumber#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.2.0",
    "puppeteer": "^3.1.0",
    "cucumber": "^6.0.5"
  },
  "bin": {
    "da-setup": "./setup.js",
    "da-takedown": "./takedown.js"
  }
}
